<script>
    var date = document.getElementById("date");

    var dt = new Date();

    var options = {
        hour12: false,
        weekday: 'long',
        year: 'numeric',
        month: 'long',
        day: 'numeric',
        hour: 'numeric',
        minute: 'numeric'
    };

    date.textContent = dt.toLocaleString('en-US', options);
</script>